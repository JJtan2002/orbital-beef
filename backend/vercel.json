{
    "builds": [{
        "src": "cashflow/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "cashflow/wsgi.py"
        }
    ],
    "regions": ["sin1"],
    "crons": [
        {
            "path": "/api/fetch-stock-data",
            "schedule": "0 0 * * *"
        }
    ]
}