{
    "builds": [
        {
            "src": "vercel-build.sh",
            "use": "@vercel/static-build"
        }
    ],
    "buildCommand": "python3 -m pip install -r requirements.txt && python3 manage.py makemigrations && python3 manage.py migrate --output dist",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cashflow/wsgi.py"
        }
    ],
    "regions": ["sin1"],
    "crons": [
        {
            "path": "/api/fetch-stock-data",
            "schedule": "0 0 * * *"
        }
    ],
    "outputDirectory": "dist"
}