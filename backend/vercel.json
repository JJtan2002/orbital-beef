{
    "version": 2,
    "builds": [
        {
            "src": "vercel-build.sh",
            "use": "@vercel/static-build",
            "config": {
                "outputDirectory": "dist/"
            }
        }
    ],
    "functions": {
        "cashflow/wsgi.py": {
            "runtime": "python3.12",
            "memory": 3009,
            "maxDuration": 30
        },
        "api/fetch-stock-data.py": {
            "runtime":"python3.12",
            "memory": 3009,
            "maxDuration": 30
        } 
    },
    "routes": [
        {
            "src": "/api/fetch-stock-data",
            "dest": "/api/fetch-stock-data.py"
        },
        {
            "src": "/(.*)",
            "dest": "cashflow/wsgi.py"
        }
    ],
    "regions": ["sin1"],
    "crons": [
        {
            "path": "/api/fetch-stock-data",
            "schedule": "0 15 * * *"
        }
    ],
    "outputDirectory": "dist/"
}